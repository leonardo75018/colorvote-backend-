{
  "kind": "collectionType",
  "collectionName": "sessions",
  "info": {
    "singularName": "session",
    "pluralName": "sessions",
    "displayName": "Session",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "datestart": {
      "type": "datetime",
      "required": true,
      "unique": true
    },
    "pin": {
      "required": true,
      "type": "biginteger",
      "unique": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "coming",
        "end",
        "inprogress"
      ],
      "default": "coming",
      "required": true
    },
    "createdby": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "sessions"
    }
  }
}
